    bindings = <&kt LCTRL>, <&kp TAB>, <&kt LCTRL>;
    ignored-key-positions = <1 45>; // LS(TAB) on _NAV layer, LG(Q) to exit app on _NAV layer, _N_NAV/esc layer key
)

ZMK_TRI_STATE(sw_app,
    bindings = <&kt LGUI>, <&kp TAB>, <&kt LGUI>; 
    ignored-key-positions = <1 45>; // LS(TAB) on _NAV layer, LG(Q) to exit app on _NAV layer, _N_NAV/esc layer key
)

